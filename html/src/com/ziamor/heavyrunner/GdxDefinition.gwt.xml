<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='com.artemis.backends.artemis_backends_gwt'/>

	<extend-configuration-property name="artemis.reflect.include" value="com.runner.heavyrunner.component"/>
	<extend-configuration-property name="artemis.reflect.include" value="com.runner.heavyrunner.system"/>
	<extend-configuration-property name="artemis.reflect.include" value="com.runner.heavyrunner.screen"/>
	<extend-configuration-property name="artemis.reflect.include" value="java.lang.Class"/>

	<extend-configuration-property name="gdx.reflect.include" value="com.runner.heavyrunner.component"/>
	<extend-configuration-property name="gdx.reflect.include" value="com.runner.heavyrunner.system"/>
	<extend-configuration-property name="gdx.reflect.include" value="com.runner.heavyrunner.screen"/>

	<inherits name='Runner' />
	<entry-point class='com.ziamor.heavyrunner.client.HtmlLauncher' />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	<set-configuration-property name="gdx.assetpath" value="../core/assets" />
</module>
